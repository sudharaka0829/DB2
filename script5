    "LOG_UTILIZATION_PERCENT":  3.73,
    "CONNECTION_STATE": "",
    "EXECUTING_APPLICATIONS": 0,
    "LOCK_WAITS_COUNT":  0,
    "ACTIVE_CONNECTIONS":  0,
    "DATABASE_SIZE_GB":  68,
    "DATABASE_STATUS": "ACTIVE ",
    "CURRENT_SERVER": "PEGADB "
}


UserParameter=db_parameter_log_utilization_percent,/bin/cat /etc/zabbix/db2_parameters.json | /usr/bin/jq -r '.[] | select(.["{#PARAMETER}"] == "LOG_UTILIZATION_PERCENT") | .Value'
